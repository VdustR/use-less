{
  "name": "react-use-less",
  "version": "0.0.3",
  "description": "useLess is useless",
  "keywords": [
    "react",
    "hook",
    "useless"
  ],
  "homepage": "https://github.com/VdustR/use-less",
  "bugs": {
    "url": "https://github.com/VdustR/use-less/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ViPro",
    "email": "VdustR@gmail.com",
    "url": "https://VdustR.github.io"
  },
  "files": [
    "cjs",
    "esm",
    "umd"
  ],
  "main": "umd/use-less.js",
  "module": "esm/use-less.js",
  "types": "esm/use-less.d.ts",
  "exports": {
    "import": "esm/use-less.js",
    "require": "cjs/use-less.js"
  },
  "type": "module",
  "scripts": {
    "build": "yarn clear && yarn build:cjs && yarn build:esm && yarn build:umd",
    "build:cjs": "tsc --project tsconfig.json --outDir cjs -m CommonJS",
    "build:esm": "tsc --project tsconfig.json --outDir esm -m ES6",
    "build:umd": "rollup -c",
    "clear": "yarn clear:build",
    "clear:build": "rimraf cjs esm umd",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VdustR/use-less.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "gitHead": "271bf631f167fc154f24be2b881744b78f292cf7"
}
